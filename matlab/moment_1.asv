x2 = randn([1,n]); %%случайная выборка
x = x1+x2;
m1 = moment(x,1) %% центральный момент первого порядка (матожидание )
m2 = moment(x,2) %% центральный момент второго порядка (дисперсия)
mi = min(x);
ma  = max(x);
